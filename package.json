    "url": "git+ssh://git@github.com/marcusasplund/sweref-convert.git"
  },
  "bugs": {
    "url": "https://github.com/marcusasplund/sweref-convert/issues"
  },
  "homepage": "https://github.com/marcusasplund/sweref-convert#readme",
  "devDependencies": {
    "babel-eslint": "^10.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "node-sass": "^4.9.3",
    "parcel-bundler": "^1.9.7",
    "standard": "*",
    "workbox-build": "^3.4.1",
    "workbox-cli": "^3.4.1"
  },

  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "dist/",
      "release/",
      "object-assign-auto.min.js",
      "promise-polyfill.js"
