{
  "name": "sweref-convert",
  "version": "0.0.4",
  "author": "Marcus Asplund <marcus@greatname.se> (@marcusasplund)",
  "license": "MIT",
  "scripts": {
    "start": "standard && parcel src/index.html -p 3000 --open",
    "build": "rm -rf release && standard && parcel build src/index.html -d release --public-url ./"
  },
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/marcusasplund/sweref-convert.git"
  },
  "bugs": {
    "url": "https://github.com/marcusasplund/sweref-convert/issues"
  },
  "homepage": "https://github.com/marcusasplund/sweref-convert#readme",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "7.2.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "babel-eslint": "^9.0.0",
    "json5": "2.1.0",
    "node-sass": "^4.9.3",
    "parcel-bundler": "^1.10.3",
    "parcel-plugin-sw-cache": "^0.2.6",
    "standard": "*"
  },
  "cache": {
    "inDev": true,
    "swDest": "service-worker.js"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "dist/",
      "release/"
    ]
  },
  "dependencies": {
    "dialog-polyfill": "^0.4.10",
    "downloadjs": "^1.4.7",
    "hyperapp": "^1.2.9",
    "leaflet": "^1.3.4",
    "papaparse": "^4.6.2",
    "promise-polyfill": "^8.1.0"
  }
}
